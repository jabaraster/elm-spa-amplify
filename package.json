{
  "name": "elm-spa-amplify",
  "version": "1.0.0",
  "scripts": {
    "build": "npx elm-spa gen && npx parcel build src/index.html",
    "dev": "npx elm-spa gen && npx parcel src/index.html --port 3333",
    "format-elm": "npx elm-format --yes src",
    "format-dev": "yarn run format-elm && yarn run dev"
  },
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-elm": "^2.9.3",
    "@parcel/transformer-image": "^2.9.3",
    "aws-amplify": "^5.3.3",
    "buffer": "^6.0.3",
    "elm": "^0.19.1-5",
    "elm-format": "^0.8.7",
    "elm-spa": "^6.0.4",
    "npm": "^9.5.0",
    "parcel": "^2.8.3",
    "punycode": "^1.4.1",
    "sharp": "^0.31.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "bulma": "^0.9.4"
  }
}